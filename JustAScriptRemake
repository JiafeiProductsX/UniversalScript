ExtraSection = SpeedWindow:NewSection("~~Extra~~")

-- WalkSpeed Changer
ExtraSection:CreateTextbox("WalkSpeed Changer", function(value)
    local adjusted = tonumber(value)
    if adjusted and adjusted >= 1 and adjusted <= 999 then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = adjusted
    end
end, {min = 1, max = 999})

-- JumpPower Changer
ExtraSection:CreateTextbox("JumpPower Changer", function(value)
    local adjusted = tonumber(value)
    if adjusted and adjusted >= 1 and adjusted <= 999 then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = adjusted
    end
end, {min = 1, max = 999})

-- Noclip Toggle
local Noclip = nil
local Clip = nil
ExtraSection:CreateButton("Noclip", function(value)
local Noclip = nil
local Clip = nil

function noclip()
	Clip = false
	local function Nocl()
		if Clip == false and game.Players.LocalPlayer.Character ~= nil then
			for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
				if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
					v.CanCollide = false
				end
			end
		end
		wait(0.21) -- basic optimization
	end
	Noclip = game:GetService('RunService').Stepped:Connect(Nocl)

	-- Send Notification when noclip is activated
	game:GetService("StarterGui"):SetCore("SendNotification", {
		Title = "~~~~~",
		Text = "Noclip Activated!",
		Duration = 3
	})
end

function clip()
	if Noclip then Noclip:Disconnect() end
	Clip = true
end

noclip() -- to toggle noclip() and clip()
end)

